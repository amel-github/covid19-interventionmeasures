
@misc{noauthor_pandemie_2020,
	title = {Pandémie de {Covid}-19 en {France}},
	copyright = {Creative Commons Attribution-ShareAlike License},
	url = {https://fr.wikipedia.org/w/index.php?title=Pand%C3%A9mie_de_Covid-19_en_France&oldid=169840247},
	abstract = {La pandémie de Covid-19 en France concerne l'évolution en France de la maladie à coronavirus 2019 (Covid-19), une maladie infectieuse émergente apparue fin 2019 en Chine continentale, dont l'agent pathogène est le coronavirus 2 du syndrome respiratoire aigu sévère (SARS-CoV-2) et dont la pandémie, déclarée le 11 mars 2020 par l'Organisation mondiale de la santé (OMS), a provoqué une crise sanitaire majeure dans le monde entier.
Le 24 janvier 2020, les trois premiers cas sont recensés sur le territoire métropolitain. Le 14 mars, l'épidémie passe au stade 3 et tous les lieux recevant du public non indispensables à la vie du pays sont fermés. À partir du 17 mars à 12 h, afin de réduire à leur plus strict minimum les contacts et les déplacements, la population est confinée à domicile sauf pour des motifs autorisés, afin de stopper la diffusion exponentielle du coronavirus et, ce faisant, réduire le plus possible le nombre de personnes atteintes et donc de morts,.
Au 20 avril, le bilan cumulatif des seuls centres hospitaliers participant au système d’information pour le suivi des victimes (Si-vic) et signalements liés à la Covid-19 dans les établissements médico-sociaux depuis le début de la pandémie, publié quotidiennement par l'Agence nationale de santé publique, fait état de 114 657 cas confirmés et 20 265 morts en France. L'Institut Pasteur a le 21 avril évalué qu'environ 5,7\% des Français (3,7 millions de personnes) auront été infectés au 11 mai (début prévu du déconfinement), précisant que pour que l'immunité collective soit suffisante pour éviter une deuxième vague, il faudrait 70\% de personnes immunisées. On est très en dessous.},
	language = {fr},
	urldate = {2020-04-21},
	journal = {Wikipédia},
	month = apr,
	year = {2020},
	note = {Page Version ID: 169840247},
	file = {Snapshot:C\:\\Users\\desvarsa\\Zotero\\storage\\T8GUHXFE\\index.html:text/html}
}

